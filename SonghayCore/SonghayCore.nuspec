<?xml version="1.0"?>
<package >
    <metadata>
        <id>SonghayCore</id>
        <version>1.4.0</version>
        <title>Songhay Core</title>
        <authors>bryan.wilhite@gmail.com</authors>
        <owners>rasx@songhaysystem.com</owners>
        <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
        <iconUrl>http://songhaysystem.com/Content/images/songhay_icon.png</iconUrl>
        <licenseUrl>https://raw.github.com/BryanWilhite/SonghayCore/master/LICENSE.md</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>core libraries meant to be shared among Songhay projects</description>
        <releaseNotes>added more throwConfigurationErrorsException support to ConfigurationManagerExtensions.</releaseNotes>
        <copyright>Copyright 2016 Bryan D. Wilhite</copyright>
        <tags>Core Songhay Configuration Cryptography LINQ</tags>
        <dependencies>
            <dependency id="Newtonsoft.Json" version="[6.0,)" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin\Release\net35\*.dll" target="lib/net35" />
        <file src="bin\Release\net35\*.exe" exclude="**vshost**" target="lib/net35" />
        <file src="bin\Release\net35\*.config" target="lib/net35" />
        <file src="bin\Release\net35\*.xml" target="lib/net35" />
        <file src="bin\Release\net35\*.pdb" target="lib/net35" />

        <file src="bin\Release\net40\*.dll" target="lib/net40" />
        <file src="bin\Release\net40\*.exe" exclude="**vshost**" target="lib/net40" />
        <file src="bin\Release\net40\*.config" target="lib/net40" />
        <file src="bin\Release\net40\*.xml" target="lib/net40" />
        <file src="bin\Release\net40\*.pdb" target="lib/net40" />

        <file src="bin\Release\net452\*.dll" target="lib/net452" />
        <file src="bin\Release\net452\*.exe" exclude="**vshost**" target="lib/net452" />
        <file src="bin\Release\net452\*.config" target="lib/net452" />
        <file src="bin\Release\net452\*.xml" target="lib/net452" />
        <file src="bin\Release\net452\*.pdb" target="lib/net452" />

        <file src="**\*.cs" exclude="obj\**" target="src" />
    </files>
</package>