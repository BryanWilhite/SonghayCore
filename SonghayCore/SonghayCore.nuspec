<?xml version="1.0"?>
<package >
  <metadata>
    <id>SonghayCore</id>
    <version>3.0.0</version>
    <title>Songhay Core</title>
    <authors>bryan.wilhite@gmail.com</authors>
    <owners>rasx@songhaysystem.com</owners>
    <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
    <iconUrl>https://songhaystorage.blob.core.windows.net/studio-public/songhay_icon.png</iconUrl>
    <licenseUrl>https://raw.github.com/BryanWilhite/SonghayCore/master/LICENSE.md</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>core libraries meant to be shared among Songhay projects</summary>
    <description>core libraries meant to be shared among Songhay projects</description>
    <releaseNotes>added mult-targeting support</releaseNotes>
    <copyright>Copyright 2018 Bryan D. Wilhite</copyright>
    <tags>Core Songhay Configuration Cryptography LINQ</tags>
    <dependencies>
        <group targetFramework=".NETFramework4.5.2">
            <dependency id="Newtonsoft.Json" version="12.0.2" />
        </group>
        <group targetFramework=".NETFramework4.6.2">
        <dependency id="Newtonsoft.Json" version="12.0.2" />
      </group>
      <group targetFramework=".NETStandard2.0">
          <dependency id="Microsoft.Extensions.Logging.TraceSource" version="2.2.0" />
          <dependency id="Newtonsoft.Json" version="12.0.2" />
        <dependency id="Tavis.UriTemplates" version="1.1.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\net452\Songhay*.dll" target="lib\net452" />
    <file src="bin\Release\net452\Songhay*.exe" exclude="**vshost**" target="lib\net452" />
    <file src="bin\Release\net452\Songhay*.config" target="lib\net452" />
    <file src="bin\Release\net452\Songhay*.xml" target="lib\net452" />

    <file src="bin\Release\net462\Songhay*.dll" target="lib\net462" />
    <file src="bin\Release\net462\Songhay*.exe" exclude="**vshost**" target="lib\net462" />
    <file src="bin\Release\net462\Songhay*.config" target="lib\net462" />
    <file src="bin\Release\net462\Songhay*.xml" target="lib\net462" />

    <file src="bin\Release\netstandard2.0\Songhay*.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\Songhay*.json" target="lib\netstandard2.0" />

    <file src="**\*.cs" exclude="**\obj\**" target="src" />
  </files>
</package>