<?xml version="1.0"?>
<package >
    <metadata>
        <id>Songhay.Net.HttpWebRequest</id>
        <version>1.0.2</version>
        <title>Songhay HttpWebRequest Extensions</title>
        <authors>bryan.wilhite@gmail.com</authors>
        <owners>rasx@songhaysystem.com</owners>
        <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
        <iconUrl>http://songhaysystem.com/Content/images/songhay_icon.png</iconUrl>
        <licenseUrl>https://raw.github.com/BryanWilhite/SonghayCore/master/LICENSE.md</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Extensions of System.Net.HttpWebRequest for legacy (net35) applications</description>
        <releaseNotes>more NuGet package cleanup.</releaseNotes>
        <copyright>Copyright 2016 Bryan D. Wilhite</copyright>
        <tags>Core Songhay HttpWebRequest</tags>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System.Xml" />
            <frameworkAssembly assemblyName="System.Xml.Linq" />
        </frameworkAssemblies>
    </metadata>
    <files>
        <file src="Songhay.Net.HttpWebRequest\bin\Release\Songhay.Net.HttpWebRequest*.dll" target="lib\net35" />
        <file src="Songhay.Net.HttpWebRequest\bin\Release\Songhay.Net.HttpWebRequest*.exe" exclude="**vshost**" target="lib\net35" />
        <file src="Songhay.Net.HttpWebRequest\bin\Release\Songhay.Net.HttpWebRequest*.config" target="lib\net35" />
        <file src="Songhay.Net.HttpWebRequest\bin\Release\Songhay.Net.HttpWebRequest*.xml" target="lib\net35" />

        <file src="Songhay.Net.HttpWebRequest\**\*.cs" exclude="Songhay.Net.HttpWebRequest\obj\**" target="src" />
    </files>
</package>