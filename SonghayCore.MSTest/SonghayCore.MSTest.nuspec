<?xml version="1.0"?>
<package >
  <metadata>
    <id>SonghayCore.MSTest</id>
    <version>3.0.1</version>
    <title>Songhay Core Testing for MSTest2 (and greater)</title>
    <authors>bryan.wilhite@gmail.com</authors>
    <owners>rasx@songhaysystem.com</owners>
    <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
    <iconUrl>https://songhaystorage.blob.core.windows.net/studio-public/songhay_icon.png</iconUrl>
    <license type="file">LICENSE.md</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Extensions of Microsoft.VisualStudio.TestTools.UnitTesting.TestContext for Songhay VSTEST projects</summary>
    <description>Extensions of Microsoft.VisualStudio.TestTools.UnitTesting.TestContext for Songhay VSTEST projects</description>
    <releaseNotes>updated Core dependencies</releaseNotes>
    <copyright>Copyright 2019 Bryan D. Wilhite</copyright>
    <tags>Core Songhay MSTEST2 VSTEST VisualStudio</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="MSTest.TestAdapter" version="1.4.0" />
        <dependency id="MSTest.TestFramework" version="1.4.0" />
        <dependency id="SonghayCore" version="3.2.2" />
      </group>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="MSTest.TestAdapter" version="1.4.0" />
        <dependency id="MSTest.TestFramework" version="1.4.0" />
        <dependency id="SonghayCore" version="3.2.2" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="MSTest.TestAdapter" version="1.4.0" />
        <dependency id="MSTest.TestFramework" version="1.4.0" />
        <dependency id="SonghayCore" version="3.2.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\net452\*.dll" target="lib\net452" />
    <file src="bin\Release\net452\*.config" target="lib\net452" />
    <file src="bin\Release\net452\*.xml" target="lib\net452" />

    <file src="bin\Release\net462\*.dll" target="lib\net462" />
    <file src="bin\Release\net462\*.config" target="lib\net462" />
    <file src="bin\Release\net462\*.xml" target="lib\net462" />

    <file src="bin\Release\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\*.deps.json" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\*.xml" target="lib\netstandard2.0" />

    <file src="**\*.cs" exclude="obj\**" target="src" />
    <file src="..\LICENSE.md" target="" />
  </files>
</package>