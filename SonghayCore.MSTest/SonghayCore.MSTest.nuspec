<?xml version="1.0"?>
<package >
  <metadata>
    <id>SonghayCore.MSTest</id>
    <version>2.2.0</version>
    <title>Songhay Core Testing for MSTest2 (and greater)</title>
    <authors>bryan.wilhite@gmail.com</authors>
    <owners>rasx@songhaysystem.com</owners>
    <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
    <iconUrl>http://songhaysystem.com/Content/images/songhay_icon.png</iconUrl>
    <licenseUrl>https://raw.github.com/BryanWilhite/SonghayCore/master/LICENSE.md</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Extensions of Microsoft.VisualStudio.TestTools.UnitTesting.TestContext for Songhay VSTEST projects</description>
    <releaseNotes>Added ShouldGetAssemblyDirectoryParent().</releaseNotes>
    <copyright>Copyright 2017 Bryan D. Wilhite</copyright>
    <tags>Core Songhay MSTEST2 VSTEST VisualStudio</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="SonghayCore" version="2.3.1" />
        <dependency id="MSTest.TestAdapter" version="1.2.0" />
        <dependency id="MSTest.TestFramework" version="1.2.0" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="SonghayCore" version="2.3.1" />
        <dependency id="MSTest.TestAdapter" version="1.1.18" />
        <dependency id="MSTest.TestFramework" version="1.1.18" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\SonghayCore.MSTest*.dll" target="lib\net452" />
    <file src="bin\Release\SonghayCore.MSTest*.config" target="lib\net452" />
    <file src="bin\Release\SonghayCore.MSTest*.xml" target="lib\net452" />

    <file src="..\SonghayCore.MSTest-netstandard2.0\bin\Release\netcoreapp2.0\SonghayCore.MSTest*.dll" target="lib\netstandard2.0" />
    <file src="..\SonghayCore.MSTest-netstandard2.0\bin\Release\netcoreapp2.0\SonghayCore.MSTest*.config" target="lib\netstandard2.0" />
    <file src="..\SonghayCore.MSTest-netstandard2.0\bin\Release\netcoreapp2.0\SonghayCore.MSTest*.xml" target="lib\netstandard2.0" />

    <file src="**\*.cs" exclude="obj\**" target="src" />
  </files>
</package>