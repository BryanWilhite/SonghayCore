<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <runtime>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <dependentAssembly>
                <assemblyIdentity name="System.Reactive.Interfaces" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
                <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0"/>
            </dependentAssembly>
            <dependentAssembly>
                <assemblyIdentity name="System.Reactive.Core" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
                <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0"/>
            </dependentAssembly>
            <dependentAssembly>
                <assemblyIdentity name="System.Reactive.Linq" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
                <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0"/>
            </dependentAssembly>
        </assemblyBinding>
    </runtime>
    <appSettings>
        <add key="environment" value="dev"/>
        <add key="dev.setting" value="the setting for DEV"/>
        <add key="qa.setting" value="the setting for QA"/>
        <add key="prod.setting" value="the setting for PROD"/>
    </appSettings>
    <connectionStrings>
        <add name="dev.Northwind" connectionString="Data Source=|DataDirectory|\Northwind.dev.sqlite" providerName="System.Data.SQLite.EF6"/>
        <add name="qa.Northwind" connectionString="Data Source=|DataDirectory|\Northwind.qa.sqlite" providerName="System.Data.SQLite.EF6"/>
        <add name="prod.Northwind" connectionString="Data Source=|DataDirectory|\Northwind.prod.sqlite" providerName="System.Data.SQLite.EF6"/>
    </connectionStrings>
</configuration>
