<?xml version="1.0"?>
<package >
  <metadata>
    <id>SonghayCore.xUnit</id>
    <version>2.0.0</version>
    <title>Songhay Core Testing for xUnit</title>
    <authors>bryan.wilhite@gmail.com</authors>
    <owners>rasx@songhaysystem.com</owners>
    <projectUrl>https://github.com/BryanWilhite/SonghayCore</projectUrl>
    <iconUrl>https://songhaystorage.blob.core.windows.net/studio-public/songhay_icon.png</iconUrl>
    <licenseUrl>https://raw.github.com/BryanWilhite/SonghayCore/master/LICENSE.md</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Extensions of DataAttribute for Songhay xUnit projects</summary>
    <description>Extensions of DataAttribute for Songhay xUnit projects</description>
    <releaseNotes>added ProjectFileDataAttribute</releaseNotes>
    <copyright>Copyright 2019 Bryan D. Wilhite</copyright>
    <tags>Core Songhay xUnit VisualStudio</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="xunit" version="2.4.1" />
        <dependency id="SonghayCore" version="2.8.8" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="SonghayCore.xUnit\bin\Release\netstandard2.0\SonghayCore.xUnit*.dll" target="lib\netstandard2.0" />
    <file src="SonghayCore.xUnit\bin\Release\netstandard2.0\SonghayCore.xUnit*.deps.json" target="lib\netstandard2.0" />
    <file src="SonghayCore.xUnit\bin\Release\netstandard2.0\SonghayCore.xUnit*.xml" target="lib\netstandard2.0" />
    <file src="SonghayCore.xUnit\**\*.cs" exclude="SonghayCore.MSTest\obj\**" target="src" />
  </files>
</package>