{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build [SonghayCore]",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SonghayCore/SonghayCore.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build [SonghayCore.MSTest]",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SonghayCore.MSTest/SonghayCore.MSTest.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build [SonghayCore.Tests]",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SonghayCore.Tests/SonghayCore.Tests.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "command": "nuget",
            "label": "NuGet pack SonghayCore",
            "isBackground": false,
            "type": "shell",
            "args": [
                "pack",
                "SonghayCore.nuspec",
                "-Verbosity",
                "detailed"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "command": "nuget",
            "label": "NuGet pack SonghayCore.MSTest",
            "isBackground": false,
            "type": "shell",
            "args": [
                "pack",
                "SonghayCore.MSTest.nuspec",
                "-Verbosity",
                "detailed"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "command": "nuget",
            "label": "NuGet pack Songhay.Net.HttpWebRequest",
            "isBackground": false,
            "type": "shell",
            "args": [
                "pack",
                "Songhay.Net.HttpWebRequest.nuspec",
                "-Verbosity",
                "detailed"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "command": "nuget",
            "label": "NuGet pack Songhay.Security",
            "isBackground": false,
            "type": "shell",
            "args": [
                "pack",
                "Songhay.Security.nuspec",
                "-Verbosity",
                "detailed"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}